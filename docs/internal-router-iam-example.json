{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowInternalLambdaRouting",
      "Effect": "Allow",
      "Action": [
        "execute-api:Invoke"
      ],
      "Resource": [
        "arn:aws:execute-api:us-east-2:824357028182:*/*/lambda/function/*"
      ],
      "Condition": {
        "StringEquals": {
          "execute-api:StageName": ["v1"]
        }
      }
    }
  ]
}